    <UseWPF>true</UseWPF>
    <ApplicationIcon>UiSon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\UiSon.Command\UiSon.Command.csproj" />
