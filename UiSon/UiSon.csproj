    <ProjectReference Include="..\UiSon.Resource\UiSon.Resource.csproj" />
    <ProjectReference Include="..\UiSon.View.Interface\UiSon.View.Interface.csproj" />
    <ProjectReference Include="..\UiSon.ViewModel\UiSon.ViewModel.csproj" />
    <ProjectReference Include="..\UiSon.View\UiSon.View.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="UiSon.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
